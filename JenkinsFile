pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                script {
                    // Check out the code from the repository
                    checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/huzaifa-123/SPM-Semester-Project.git']]])
                }
            }
        }

    



    post {
        always {
            // Clean up or perform any other actions after the pipeline
            echo 'CI Pipeline completed!'
        }
    }
}
